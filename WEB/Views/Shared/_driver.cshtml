<div class="row" ng-controller="driverController">

    <div class="row">
        <label>{{result}}</label>
    </div>
    <label>{{pageName}}</label>
    <br />
    <div class="row">
        <label>User Name</label>
        <input type="text" ng-model="resource.Name" />

    </div>
    <div class="row">
        <label>DOB</label>
        <input type="text" ng-model="resource.Dob" />

    </div>
    <div class="row">
        <label>Marital Status</label>
        <input type="text" ng-model="resource.MaritalStatus" />

    </div>
    <div class="row">
        <input type="button" value="Save" ng-click="save();" />
    </div>
    @*<div>{{yearLookup}}</div>*@
</div>

